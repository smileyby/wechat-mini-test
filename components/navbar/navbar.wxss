/* component/navbar/navbar.wxss */
.navbar {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  color: #000;
  z-index: 9999999;
  background: #01A08A;
  text-align: center;
  box-sizing: border-box;
}

/* search start */
.nav-search {
  position: absolute;
  display: flex;
  align-items: center;
  justify-content: center;
  border: 1px solid #f4f4f4;
  font-size: 15px;
  background: #f4f4f4;
}

