/* nav-back start */
.nav-back {
  position: absolute;
  width: 12px;
  height: 24px;
}
/* nav-back end */

/* nav-home start */
.nav-home {
  position: absolute;
  width: 24px;
  height: 24px;
}
/* nav-home end */

/* nav-back-home-group start */
.nav-back-home-group {
  position: absolute;
  display: flex;
  padding: 10px;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  border: 1rpx solid #f0f0f0;
  background-color: rgba(234, 234, 234, 0.6);
}
.nav-back-home-group:before {
  content: ' ';
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  width: 1rpx;
  height: 18.4px;
  background: #e0e0e0;
}
.nav-back-home-group .group-back {
  width: 12px;
  height: 24px;
}
.nav-back-home-group .group-home {
  width: 24px;
  height: 24px;
}
/* nav-back-home-group end */